# Application Environment
VITE_APP_ENV=development
VITE_APP_NAME=Whaledone
VITE_APP_VERSION=0.1.0

# API Configuration
VITE_API_BASE_URL=http://localhost:3001/api
VITE_API_TIMEOUT=30000

# Authentication
VITE_AUTH_TOKEN_KEY=whaledone_token
VITE_AUTH_REFRESH_TOKEN_KEY=whaledone_refresh_token

# Social Login
VITE_GOOGLE_CLIENT_ID=your_google_client_id
VITE_KAKAO_CLIENT_ID=your_kakao_client_id
VITE_APPLE_CLIENT_ID=your_apple_client_id

# Feature Flags
VITE_ENABLE_DEBUG=false
VITE_ENABLE_MOCK_DATA=true
VITE_ENABLE_ANALYTICS=false

# Storage
VITE_STORAGE_PREFIX=whaledone_
VITE_LANGUAGE_STORAGE_KEY=whaledone_language
VITE_VISITED_STORAGE_KEY=whaledone_visited
